  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:both:ci": "ng build --base-href \"https://koderlabs.github.io/ngx-device-detector/demo/\" --prod --output-hashing=all && npm run build:server",
    "build:server": "ng run ngx-device-detector-demo:server:production --bundleDependencies all",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && npm run build:server",
    "dev:ssr": "ng run ngx-device-detector-demo:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run ngx-device-detector-demo:server:production",
    "prerender": "ng run ngx-device-detector-demo:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.5",
